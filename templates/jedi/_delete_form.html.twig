<form method="post" action="{{ path('jedi_delete', {'id': jedi.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ jedi.id) }}">
    <button class="btn">Delete</button>
</form>
